{
  "name": "server-node",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": "14.15.1",
    "yarn": "1.22.5"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "yarn --cwd ./client start",
    "dev": "concurrently \"yarn run server\" \"yarn run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false yarn install --cwd client && yarn --cwd ./client build"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cookie-session": "^1.4.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.11",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "sendgrid": "^5.2.3",
    "stripe": "^8.131.0"
  }
}
